{
  "name": "judge-ca",
  "version": "1.0.0",
  "description": "Attorney referral service and advertising platform for Quebec",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/backend/server.ts",
    "dev:frontend": "next dev",
    "build": "next build",
    "build:frontend": "next build",
    "build:backend": "tsc -p tsconfig.server.json",
    "start": "node dist/backend/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "validate": "npm run lint && npm run typecheck && npm test",
    "prepare": "husky install",
    "deploy:staging": "node -e \"require('./src/backend/services/deployment-agent.service').backendDeploymentAgent.deployBackend({platform: 'vercel', environment: 'staging'})\"",
    "deploy:production": "node -e \"require('./src/backend/services/deployment-agent.service').backendDeploymentAgent.deployBackend({platform: 'vercel', environment: 'production'})\"",
    "deploy:docker": "node -e \"require('./src/backend/services/deployment-agent.service').backendDeploymentAgent.deployBackend({platform: 'docker', environment: 'production'})\"",
    "deploy:health": "curl -f https://judgeca-axaiinovation.vercel.app/api/health || echo 'Health check failed'",
    "deployment:start": "ts-node src/backend/server.ts"
  },
  "keywords": [
    "attorney",
    "legal",
    "referral",
    "quebec",
    "canada"
  ],
  "author": "Judge.ca",
  "license": "UNLICENSED",
