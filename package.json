{
  "name": "judge-ca",
  "version": "1.0.0",
  "description": "Attorney referral service and advertising platform for Quebec",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/backend/server.ts",
    "dev:frontend": "next dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "next build",
    "build:backend": "tsc -p tsconfig.server.json",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run"
  },
  "keywords": ["attorney", "legal", "referral", "quebec", "canada"],
  "author": "Judge.ca",
  "license": "UNLICENSED",
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "marked": "^11.1.1",
    "multer": "^2.0.2",
    "next": "^14.1.0",
    "next-i18next": "^15.2.0",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "stripe": "^14.13.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.33",
    "ts-node": "^10.9.2"
  }
}